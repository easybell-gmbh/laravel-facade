{
    "name": "hmones/laravel-facade",
    "description": "Generates facades in your laravel project with one simple command.",
    "license": "MIT",
    "authors": [
        {
            "name": "Haytahm Mones",
            "email": "haytham.mones@gmail.com",
            "homepage": "https://www.haythammones.com"
        },
        {
            "name": "Maab Javaid",
            "email": "maabjavaid9@gmail.com"
        }
    ],
    "homepage": "https://github.com/hmones/laravel-facade",
    "keywords": [
        "Laravel",
        "LaravelFacade"
    ],
    "minimum-stability": "stable",
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require": {
        "php": "^7.4|^8.0",
        "composer-runtime-api": "^2.0",
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Hmones\\LaravelFacade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hmones\\LaravelFacade\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hmones\\LaravelFacade\\LaravelFacadeServiceProvider"
            ],
            "aliases": {
                "LaravelFacade": "Hmones\\LaravelFacade\\Facades\\LaravelFacade"
            }
        }
    }
}
